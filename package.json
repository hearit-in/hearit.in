{
  "name": "hearit",
  "author": {
    "name": "Sebastian Reinhard",
    "email": "sebastian@sebbert.me",
    "url": "https://github.com/sebbert"
  },
  "private": true,
  "version": "0.0.2",
  "description": "Front-end for a music queuing system",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/404.html ./src/favicon.ico ./src/favicon/* ./src/manifest.json ./dist",
    "dist": "npm run copy && webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "firebase-rules": "firebase-bolt < cfg/rules.bolt > cfg/rules.json",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "predeploy": "npm run dist && npm run firebase-rules",
	"deploy": "sh ./scripts/deploy.sh",
    "deploy:rules": "npm run firebase-rules && firebase deploy --only database"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebbert/crowdplay-web.git"
  },
  "keywords": [],
  "david": {
    "ignore": [
      "react"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.0.15",
    "chai": "^3.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "firebase-bolt": "^0.8.1",
    "glob": "^7.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "nib": "~1.0.4",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.0.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.2.9",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.4.3",
    "serviceworker-webpack-plugin": "^0.1.4",
    "style-loader": "^0.13.0",
    "stylus": "~0.49.2",
    "stylus-loader": "^0.5.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "core-js": "^2.0.0",
    "firebase": "^3.1.0",
    "history": "^2.0.2",
    "immutable": "^3.8.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.11.1",
    "material-ui": "^0.14.4",
    "normalize.css": "^4.0.0",
    "q": "^1.4.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-flip-move": "^2.1.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-tap-event-plugin": "^0.2.0",
    "redux": "^3.4.0",
    "redux-actions": "^0.9.1",
    "redux-immutable": "^3.0.6",
    "redux-localstorage": "^1.0.0-rc",
    "redux-localstorage-filter": "^0.1.1",
    "redux-localstorage-immutable": "^1.0.4",
    "redux-thunk": "^2.0.1",
    "velocity-react": "^1.1.5"
  }
}
